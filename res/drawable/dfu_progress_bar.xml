<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10.0dip" />
            <gradient android:startColor="#ff3c3c3b" android:endColor="#ff3c3c3b" android:centerColor="#ff3c3c3b" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="10.0dip" />
                <gradient android:startColor="#ff3c3c3b" android:endColor="#ff3c3c3b" android:centerColor="#ff3c3c3b" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10.0dip" />
                <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:centerColor="#ffffffff" />
            </shape>
        </clip>
    </item>
</layer-list>