<?xml version="1.0" encoding="utf-8"?>
<selector android:enterFadeDuration="250" android:exitFadeDuration="250"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="false">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="0.0dip" android:thickness="4.0dip">
            <solid android:color="@color/goalGray" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="0.0dip" android:thickness="4.0dip">
            <solid android:color="?helpCardTheme" />
        </shape>
    </item>
</selector>